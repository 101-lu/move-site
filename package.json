{
  "name": "move-site",
  "version": "0.1.0",
  "description": "CLI utility that help to move sites between hosts",
  "type": "module",
  "bin": {
    "move-site": "./dist/bin/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/bin/cli.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "wordpress",
    "ssh",
    "sftp",
    "deployment",
    "website",
