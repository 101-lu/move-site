{
  "name": "move-site",
  "version": "2.0.0",
  "description": "CLI utility that help to move sites between hosts",
  "keywords": [
    "cli",
    "wordpress",
    "ssh",
    "sftp",
    "deployment",
    "website",
    "migration"
  ],
  "homepage": "https://github.com/101-lu/move-site#readme",
  "bugs": {
    "url": "https://github.com/101-lu/move-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/101-lu/move-site.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "license": "MIT",
  "author": "Andrey Lechev @ 101 Studios",
